#include <stdio.h>
int main() {
    // Declare a set of numbers
    int numbers[] = {10, 23, 45, 66, 77, 90};
    int size = sizeof(numbers) / sizeof(numbers[0]);

    printf("Odd and Even Numbers:\n");

    for (int i = 0; i < size; i++) {
        if (numbers[i] % 2 == 0) {
            printf("%d is Even\n", numbers[i]);
        } else {
            printf("%d is Odd\n", numbers[i]);
        }
    }
    return 0;
}
